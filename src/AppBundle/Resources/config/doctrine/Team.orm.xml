<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Team" table="team">
    <indexes>
      <index name="fk_team_sexe1_idx" columns="sexe_id"/>
      <index name="fk_team_category1_idx" columns="category_id"/>
      <index name="fk_team_division1_idx" columns="division_id"/>
      <index name="fk_team_season1_idx" columns="season_id"/>
      <index name="fk_team_club1_idx" columns="club_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="category" target-entity="Category" fetch="LAZY">
      <join-columns>
        <join-column name="category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="club" target-entity="Club" fetch="LAZY">
      <join-columns>
        <join-column name="club_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="division" target-entity="Division" fetch="LAZY">
      <join-columns>
        <join-column name="division_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="season" target-entity="Season" fetch="LAZY">
      <join-columns>
        <join-column name="season_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sexe" target-entity="Sexe" fetch="LAZY">
      <join-columns>
        <join-column name="sexe_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
