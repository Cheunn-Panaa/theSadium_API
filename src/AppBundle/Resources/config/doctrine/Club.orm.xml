<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Club" table="club">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationDate" type="string" column="creation_date" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zipCode" type="string" column="zip_code" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adress" type="string" column="adress" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="profilePicture" type="string" column="profile_picture" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coverPhoto" type="string" column="cover_photo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="trainingHours" type="text" column="training_hours" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
